package locker;

public class OpenedLockers {

	public static void main(String[] args) 
	{
		boolean lockers[]  = new boolean[100];
		
		for (int i = 0; i < lockers.length; i++) {
            lockers[i] = true;
        }
		
		for (int i = 1; i < lockers.length; i++) {
            lockers[i] = false;
        }
	}		
	}
