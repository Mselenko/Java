package task_2;

import task_1.MyDate;

public class Employee extends Person 
{

	String office;
	long salary;
	MyDate hiredate;
	
	public Employee() 
	{
		super();
	}
	
	public Employee(String office, long salary, MyDate hiredate) 
	{
		super();
		this.office = office;
		this.salary = salary;
		this.hiredate = hiredate;
		
	}
	public String toString() 
	{
		return super.toString();
	}	
}
